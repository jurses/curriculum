<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="view" prefHeight="425.0" prefWidth="679.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <rowConstraints>
      <RowConstraints maxHeight="379.0" minHeight="0.0" prefHeight="33.0" vgrow="ALWAYS" />
      <RowConstraints maxHeight="402.0" minHeight="0.0" prefHeight="389.0" vgrow="ALWAYS" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" maxWidth="611.0" minWidth="0.0" prefWidth="591.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="299.0" minWidth="0.0" prefWidth="80.0" />
   </columnConstraints>
   <children>
      <TableView fx:id="tableFormacion" prefHeight="200.0" prefWidth="200.0" GridPane.rowSpan="2">
        <columns>
          <TableColumn fx:id="columnDesde" prefWidth="75.0" text="Desde" />
          <TableColumn fx:id="columnHasta" prefWidth="75.0" text="Hasta" />
            <TableColumn fx:id="columnDenominacion" prefWidth="96.0" text="Denominación" />
            <TableColumn fx:id="columnOrganizador" prefWidth="90.0" text="Organizador" />
        </columns>
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin>
      </TableView>
      <Button fx:id="btnAgregar" mnemonicParsing="false" onAction="#agregarFormacion" text="Añadir" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="btnEliminar" mnemonicParsing="false" onAction="#eliminarTitulo" text="Eliminar" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </GridPane.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
</GridPane>
